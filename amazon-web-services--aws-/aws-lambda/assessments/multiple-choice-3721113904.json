		"answers": [
			{
				"_id": "8f82d6aa-e4d6-4563-777a-6f44a89cc2e4",
				"correct": false,
				"answer": "The workdir should be `/app`."
			},
			{
				"_id": "565e36bc-555f-d584-91e0-f15bf61d3aaa",
				"correct": true,
				"answer": "The base image `python:3.10.15` doesn't exist."
			},
			{
				"_id": "455414e8-0626-3815-be1e-06261f2ccab6",
				"correct": true,
				"answer": "Docker doesn't allow copying files from outside the build context (`../`)"
			},
			{
				"_id": "008d238b-c5a3-1195-eb10-a0792ed456d3",
				"correct": false,
				"answer": "The `CMD` should be `CMD \"python app.py\"`."
